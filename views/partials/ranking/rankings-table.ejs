<div data-id='rankings-table' class='general-sub-section'>
	<div class='general-categories-container'>
		<h2 class='general-category-year-header'><span class='general-category'><%- functions.capitalizeFirstLetter(rankings.category) %></span> <span class='general-year'> - <%- rankings.yearName %></span></h2>
		<div class='tabs'>
			<div data-rankings-type='oom' class='tab-title <%- (rankings.showType == "oom" ? " active " : "") %>' onclick="showOnRankings('oom', $('[data-id=rankings-year]').val(), $('[data-id=rankings-category]').val(), <%- (typeof replaceHistory !== "undefined" && replaceHistory ? "true" : "false") %>);">Order of Merit</div>
			<div data-rankings-type='money' class='tab-title <%- (rankings.showType == "money" ? " active " : "") %>' onclick="showOnRankings('money', $('[data-id=rankings-year]').val(), $('[data-id=rankings-category]').val(), <%- (typeof replaceHistory !== "undefined" && replaceHistory ? "true" : "false")%>);">Money</div>
		</div>
	</div>
	<div data-rankings-type='oom' class='rankings-oom-section rankings-type-section <%- (rankings.showType == "oom" ? " active " : "") %>'>
		<table class='general-table'>
			<thead>
				<th class='rankings-table-header'>Rank</th>
				<th class='rankings-table-header'>Team</th>
				<th class='rankings-table-header'>YTD</th>
				<th class='rankings-table-header'>Total</th>

			</thead>
			<tbody>
				<%
				for (var team in rankings.rankings) { %>
					<tr class='general-table-row'>
						<td class='rankings-table-data rankings-table-data-rank'><%- rankings.rankings[team].rank %></td>
						<td class='rankings-table-data rankings-table-data-name'><a href='/teams/<%- rankings.rankings[team].teamId %>'><%- rankings.rankings[team].name %></a> <span class='rankings-table-data-location'> <span class='hidden-mini-tablets'>-</span> <%- rankings.rankings[team].location %></span></td>
						<td class='rankings-table-data rankings-table-data-points'><%- rankings.rankings[team].ytdPoints.toFixed(3) %></td>
						<td class='rankings-table-data rankings-table-data-points'><%- rankings.rankings[team].totalPoints.toFixed(3) %></td>
					</tr>
				<% } %>
			</tbody>
		</table>
	</div>
	<div data-rankings-type='money' class='rankings-money-section rankings-type-section <%- (rankings.showType == "money" ? " active " : "") %>'>
		<table class='general-table'>
			<thead>
				<th class='rankings-table-header'>Rank</th>
				<th class='rankings-table-header'>Team</th>
				<th class='rankings-table-header'>Total</th>

			</thead>
			<tbody>
				<%
				for (var team in rankings.money) { %>
					<tr>
						<td class='rankings-table-data rankings-table-data-rank'><%- rankings.money[team].rank %></td>
						<td class='rankings-table-data rankings-table-data-name'><a href='/teams/<%- rankings.money[team].teamId %>'><%- rankings.rankings[team].name %></a> <span class='rankings-table-data-location'> <span class='hidden-mini-tablets'>-</span> <%- rankings.rankings[team].location %></span></td>
						<td class='rankings-table-data rankings-table-data-points'>$<%- functions.numberWithCommas(rankings.money[team].money); %></td>
					</tr>
				<% } %>
			</tbody>
		</table>
	</div>
</div>
