<section class='scores-section section'>
	<h2 class='section-header'>Scores</h2>
	<section class='sub-section'>
		<div class='scores-sub-section'>
			<% if (competitions != null && competitions.length > 0) { %>
				<% include scoreboard/categories %>
				<% include scoreboard/scores-all %>
			<% } else {%>
				<h3 class='no-games-text'>It appears there are no events to show at this current moment</h3>
			<% } %>
		</div>
	</section>
</section>