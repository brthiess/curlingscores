<div class='scores-container' data-competition-id='<%= competition.competitionId %>'>
	<div class='scores-header'>
		<h2 class='scores-header-title'><%= competition.name %></h2> 
		<div class='scores-tabs'>
			<h3 class='scores-tab-title active' data-scoreboard-section='scores' onclick='showOnScoreboard("scores");'>Scores</h3>
			<h3 class='scores-tab-title' data-scoreboard-section='standings' onclick='showOnScoreboard("standings");'>Standings</h3>
			<h3 class='scores-tab-title' data-scoreboard-section='bracket' onclick='showOnScoreboard("bracket");'>Bracket</h3>
		</div>
	</div>
	<% include scores %>
	<% include standings %>
	
</div>