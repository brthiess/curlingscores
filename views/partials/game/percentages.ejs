<table class='game-team-details-table'>
	<thead>
		<th class='game-team-details-table-header t-left'></th>
		<th class='game-team-details-table-header t-left'>Team <%- game.team1.name %></th>
		<th class='game-team-details-table-header t-center'>PCT</th>
	</thead>
	<tbody>
		<% for (var player in game.team1.players) { %>
			<tr>
				<td class='game-team-details-table-data t-left'><img src='<%- game.team1.players[player].image %>'/></td>
				<td class='game-team-details-table-data t-left'><%- game.team1.players[player].name %><span class='game-team-details-player-position'><%- game.team1.players[player].position %></span></td>
				<td class='game-team-details-table-data t-center'><%- Math.floor(Math.random() * 30) + 70   %>%</td>
			</tr>
		<% } %>
	</tbody>
</table>
</div>
